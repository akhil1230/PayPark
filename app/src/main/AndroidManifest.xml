    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature android:name="android.hardware.location.gps" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/parking"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.akhil.nikhil.paypark.start_logo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.akhil.nikhil.paypark.MainActivity" />
        <activity android:name="com.akhil.nikhil.paypark.create_newactivity" />
        <activity android:name="com.akhil.nikhil.paypark.create_sp" />
        <activity android:name="com.akhil.nikhil.paypark.sp_login" />
        <activity android:name="com.akhil.nikhil.paypark.user_home_page" />
        <activity android:name="com.akhil.nikhil.paypark.pay" />
        <activity android:name="com.akhil.nikhil.paypark.sp_services" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCtGeTLwTNYf6DLhizLolnRDHGkGt5RKWg" />

        <activity android:name="com.akhil.nikhil.paypark.Start_activity"></activity>
    </application>

</manifest>